{
  "name": "swap-vm",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "typescript/swap-vm/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "typescript/swap-vm",
        "commands": ["tsdown"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["typescript/swap-vm/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "typescript/swap-vm/vitest.config.mts"
      }
    }
  }
}
